
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title>FBCOM Management System</title>
    <meta content="" name="description">
    <meta content="" name="keywords">

    <!-- Favicons -->
    <link href="~/img/favicon.png" rel="icon">
    <link href="~/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.gstatic.com" rel="preconnect">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/vendor/quill/quill.snow.css" rel="stylesheet">
    <link href="~/vendor/quill/quill.bubble.css" rel="stylesheet">
    <link href="~/vendor/remixicon/remixicon.css" rel="stylesheet">
    <link href="~/vendor/simple-datatables/style.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="~/css/style.css" rel="stylesheet">
</head>

<body>

    <!-- ======= Header ======= -->
    <header id="header" class="header fixed-top d-flex align-items-center">

        <div class="d-flex align-items-center">
            <a class="logo d-flex align-items-center">
                <span class="d-none d-lg-block">FBCCOM Management System</span>
            </a>
            <i class="bi bi-list toggle-sidebar-btn"></i>
        </div><!-- End Logo -->




    </header>
    <!-- End Header -->
    <!-- ======= Sidebar ======= -->
    <aside id="sidebar" class="sidebar">

        <ul class="sidebar-nav" id="sidebar-nav">


            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="Index" asp-controller="Home">
                    <i class="bi bi-house-door-fill"></i>
                    <span> Acceuil</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="Index" asp-controller="CPE">
                    <i class="bi bi-router-fill"></i>
                    <span> Equiments CPE</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="Index" asp-controller="Telephone">
                    <i class="bi bi-telephone-inbound-fill"></i>
                    <span> Telephones</span>
                </a>
            </li>

            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="IndexPreinstallation" asp-controller="Planning">
                    <i class="bi bi-person-rolodex"></i>
                    <span>Planning</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="Index" asp-controller="Installation">
                    <i class="bi bi-file-earmark-spreadsheet-fill"></i>

                    <span >Installations</span>
                </a>
            </li>

            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="IndexNegative" asp-controller="Installation">
                    <i class="bi bi-file-earmark-excel-fill"></i>

                    <span>SINR Négative</span>
                </a>
            </li>

            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="CreateMatriceView" asp-controller="Installation">
                    <i class="bi bi-save-fill"></i>

                    <span>Matrice</span>
                </a>
            </li>


            <li class="nav-item">
                <a class="nav-link collapsed" asp-action="backUp" asp-controller="Installation">
                    <i class="bi bi-cloud-arrow-up-fill"></i>

                    <span>backUp</span>
                </a>
            </li>








            <!-- End Users Nav -->

        @*        <li class="nav-item">
                    <form asp-action="Logout" asp-controller="Account" method="post">
                        <button type="submit" class="btn btn-outline bi bi-cart-fill nav-link collapsed">Logout</button>


                    </form>
                </li>*@



        </ul>

    </aside>
    <!-- End Sidebar-->

    <main role="main" id="main" class="main">
        @RenderBody()
    </main>



    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    <script src="~/vendor/apexcharts/apexcharts.min.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/vendor/chart.js/chart.min.js"></script>
    <script src="~/vendor/echarts/echarts.min.js"></script>
    <script src="~/vendor/quill/quill.min.js"></script>
    <script src="~/vendor/simple-datatables/simple-datatables.js"></script>
    <script src="~/vendor/tinymce/tinymce.min.js"></script>
    <script src="~/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/js/main.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>